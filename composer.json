{
  "require": {
    "craftcms/ckeditor": "1.0.0-beta.3",
    "craftcms/cms": "3.5.12.1",
    "craftcms/redactor": "2.8.1",
    "fortrabbit/craft-copy": "1.0.0-RC12",
    "mmikkel/reasons": "^2.2",
    "mwikala/linode-s3": "1.0.1",
    "symfony/console": "^4.1",
    "verbb/navigation": "1.3.31",
    "verbb/super-table": "2.6.3",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
