{
  "require": {
    "craftcms/ckeditor": "1.0.0-beta.3",
    "craftcms/cms": "^3.4.25",
    "craftcms/redactor": "2.7.1",
    "mmikkel/reasons": "^2.2",
    "mwikala/linode-s3": "1.0.0",
    "verbb/navigation": "1.3.27",
    "verbb/super-table": "2.6.2",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
